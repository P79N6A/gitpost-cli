{
  "name": "gitpost-cli",
  "description": "gitpost command line",
  "version": "0.0.0",
  "author": "xiaomingplus",
  "bin": {
    "gitpost": "./bin/run"
  },
  "bugs": "https://github.com/xiaomingplus/gitpost-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/cpy": "^5.1.0",
    "@types/fs-extra": "^5.0.4",
    "@types/make-dir": "^1.0.3",
    "@types/yeoman-generator": "^3.0.1",
    "cpy": "^7.0.1",
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1",
    "hexo-cli": "^1.1.0",
    "make-dir": "^1.3.0",
    "minimist": "^1.2.0",
    "sort-pjson": "^1.0.3",
    "tslib": "^1",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://github.com/xiaomingplus/gitpost-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "gitpost",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "xiaomingplus/gitpost-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
